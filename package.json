{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@types/react-helmet": "^5.0.15",
    "bootstrap": "^4.4.1",
    "gatsby": "^2.21.9",
    "gatsby-background-image": "^0.9.19",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-postcss": "^2.3.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-sharp": "^2.5.0",
    "intersection-observer": "^0.7.0",
    "node-sass": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "resolve-url-loader": "^3.1.1",
    "tailwindcss": "^1.4.4",
    "typeface-open-sans": "0.0.75",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "destiny": "^0.6.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^1.19.1",
    "prettier-standard": "^16.3.0",
    "tailwindcss-dark-mode": "^1.1.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,md}": "prettier"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && destiny -w \"src/components/*.*\""
    }
  }
}
